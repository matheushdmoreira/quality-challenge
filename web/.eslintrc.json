{
  "extends": "@rocketseat/eslint-config/react",
  "plugins": ["simple-import-sort"],
  "rules": {
    "@typescript-eslint/no-unsafe-declaration-merging": "off",
    "simple-import-sort/imports": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "camelcase": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}